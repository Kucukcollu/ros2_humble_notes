{
    "env": {
        "myIncludePath": ["${workspaceFolder}/include", "${workspaceFolder}/src"]
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/ros/humble/include/**",
                "/usr/include/eigen3"
            ],
            "defines": [],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/g++",
            "compilerArgs": [
                "-std=c++17",
                "-Wpedantic",
                "-Wall",
                "-Wextra",
                "-Werror",
                "-Wunused",
                "-Wconversion",
                "-Wsign-conversion",
                "-O2",
                "-g",
                "-fsanitize=address",
                "-fsanitize=undefined",
                "-fanalyzer",
                "-fdiagnostics-show-option"
            ],
            "browse": {
                "path": ["${workspaceFolder}/**", "/usr/include", "/usr/local/include"],
                "limitSymbolsToIncludedHeaders": true
              }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
